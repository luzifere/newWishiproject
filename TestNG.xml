<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" allow-return-values="true" >
    <listeners>
        <listener class-name="utilitis.Listener"/>
    </listeners>
    <test verbose="1" thread-count="1" preserve-order="true" name="Test" parallel="classes">
        <groups>
            <run>
                <include name="sanity-group"/>
            </run>
        </groups>
       <classes>

           <class name="TestsObjects.SignUpTest"/>
           <class name="TestsObjects.OnBoardingFemaleTest"/>
           <class name="TestsObjects.LoginTest"/>
           <class name="TestsObjects.OnBoardingMaleTest"/>
           <class name="TestsObjects.OnBoardingNaturalTest">
           </class>
       </classes>
    </test>
</suite>