<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" allow-return-values="true" >
    <listeners>
        <listener class-name="TestsObjects.Listener"></listener>
    </listeners>
    <test verbose="10" thread-count="5" preserve-order="true" name="Test" parallel="classes">
        <groups>
            <run>
                <include name="sanity-group"/>
            </run>
        </groups>
       <classes>
           <class name="TestsObjects.LoginTest"/>
           <class name="TestsObjects.SignUpTest">
           </class>
       </classes>
    </test>
</suite>